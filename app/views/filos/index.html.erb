
    <h2>Filo</h2>
    <form id="FormBusca">
    Novo Filo:
    <input type="button" id="maisFilo" value="+" onclick="javascript:window.location='/filos/new'" />
    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    Busca:
    <input id="txtNome" type="text" />
    <input id="btnBusca" type="button" value="OK" />
    </form>
    <br />

    <table id="tb2" border="1" cellpadding="0" cellspacing="0" class="table_geral">
        <thead>
            <tr>
                <th width="225px">
                    Descrição
                </th>
                <th width="225px">
                    Status
                </th>
            </tr>
        </thead>
        <tbody>
            <tr class="odd">
			<% @filos.each do |filo| %>
                <td width="225px">
                    <%= link_to filo.descricao, edit_filo_path(filo) %>
                </td>
                <td width="225px">
                    <%= filo.status %>
                </td>
            </tr>
			<% end %>
        </tbody>
    </table>
