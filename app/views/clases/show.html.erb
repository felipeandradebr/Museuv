<div id="box">
    	<h4 align="center"><hr />Classe<hr /></h4>
    			<form>
    			<table cellspacing="0" id="tab">
                    <tr>
                        <td>Filo:</td>
						<% @filos.each do |filo| %>
							<% if @clase.filo_id == filo.id %>
								<td><%= filo.descricao %></td>
							<% end %>
						<% end %>
                    </tr>
                    <tr>
            			<td><b>Classe:</b></td>
            			<td><b>  <%= @clase.descricao %></b></td>
            		</tr>
                	<tr>
                        <td></td>
                        <td></td>
    				</tr>
					<tr>
                        <td><%= link_to image_tag("/assets/voltar.png", :size => "24x24"), clases_path %></td>
                        <td style="width:900px;"></td>
        			</tr>
    			</table>
    			</form>
    </div>