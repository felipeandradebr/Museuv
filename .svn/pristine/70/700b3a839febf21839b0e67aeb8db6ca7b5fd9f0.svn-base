<h1>Exemplares</h1>

<table>
  <tr>
    <th>Nome vulgar</th>
    <th>Nome cientifico</th>
    <th>Codigo identificacao</th>
    <th>Local origem</th>
    <th>Localizacao geografica</th>
    <th>Identificador</th>
    <th>Data coleta</th>
    <th>Data chegada</th>
    <th>Foto costas</th>
    <th>Foto dorso</th>
    <th>Localizacao exemplar</th>
    <th>Biografia</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exemplars.each do |exemplar| %>
  <tr>
    <td><%= exemplar.nome_vulgar %></td>
    <td><%= exemplar.nome_cientifico %></td>
    <td><%= exemplar.codigo_identificacao %></td>
    <td><%= exemplar.local_origem %></td>
    <td><%= exemplar.localizacao_geografica %></td>
    <td><%= exemplar.identificador %></td>
    <td><%= exemplar.data_coleta %></td>
    <td><%= exemplar.data_chegada %></td>
    <td><%= exemplar.foto_costas %></td>
    <td><%= exemplar.foto_dorso %></td>
    <td><%= exemplar.localizacao_exemplar %></td>
    <td><%= exemplar.biografia %></td>
    <td><%= exemplar.status %></td>
    <td><%= link_to 'Consultar', exemplar %></td>
    <td><%= link_to 'Editar', edit_exemplar_path(exemplar) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Novo Exemplar', new_exemplar_path %>
